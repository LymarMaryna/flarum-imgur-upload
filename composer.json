{
    "name": "lymarmaryna/flarum-aws-upload",
    "description": "Simple AWS image upload from the Flarum editor",
    "type": "flarum-extension",
    "license": "GPL-3.0",
    "authors": [
      {
        "name": "Matteo Contrini",
        "email": "matteo@contrini.it"
      }
    ],
    "require": {
        "flarum/core": "^1.0.0",
        "league/flysystem-aws-s3-v3": "1.*"
    },
    "extra": {
        "flarum-extension": {
            "title": "AWS Image Upload ‚òÅ\uFE0F",
            "category": "feature",
          "icon": {
            "name": "fas fa-cloud-upload-alt",
            "backgroundColor": "#232F3E",
            "color": "#FF9900"
          },
            "optional-dependencies": ["flarum/markdown"]
        }
    },
    "support": {
        "issues": "https://github.com/lymarmaryna/flarum-imgur-upload/issues",
        "source": "https://github.com/lymarmaryna/flarum-imgur-upload",
        "forum": "https://discuss.flarum.org/d/18491"
    },
    "autoload": {
        "psr-4": {
            "ImgurUpload\\": "src/"
        }
    }
}
